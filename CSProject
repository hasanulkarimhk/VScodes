                        tprice= qtynum*price;
                        printf("=======Details of  The Order=======\nUnique Code: %s \nTeam Name: %s \nJersey Quality: %s \nJersey Design: %s \nJersey Size: %s \nQuantity: %d \nTotal Price: %d TAKA \nOrder Time: %d/%d/%d %02d:%02d\n\n", str2, teamjersey,jerseyquality, jerseydesigntype, jerseysize, qtynum, tprice, local_time->tm_mon + 1, local_time->tm_mday, local_time->tm_year + 1900, local_time->tm_hour, local_time->tm_min);
                        sprintf(str3,"=======Details of  The Order=======\nUnique Code: %s \nTeam Name: %s \nJersey Quality: %s \nJersey Design: %s \nJersey Size: %s \nQuantity: %d \nTotal Price: %d TAKA \nOrder Time: %d/%d/%d %02d:%02d\n\n", str2, teamjersey,jerseyquality, jerseydesigntype, jerseysize, qtynum, tprice, local_time->tm_mon + 1, local_time->tm_mday, local_time->tm_year + 1900, local_time->tm_hour, local_time->tm_min);



                    }
                }
                else if(sizenum==4){

                    printf("Size: Large");
                    strcpy(jerseysize, "Large.");
                    printf("\n\nEnter The Quantity: ");
                    scanf("%d", &qtynum);
                    if(qtynum>0){

                        time_t current_time = time(NULL);
                        struct tm *local_time = localtime(&current_time);
                        tprice= qtynum*price;
                        printf("=======Details of  The Order=======\nUnique Code: %s \nTeam Name: %s \nJersey Quality: %s \nJersey Design: %s \nJersey Size: %s \nQuantity: %d \nTotal Price: %d TAKA \nOrder Time: %d/%d/%d %02d:%02d\n\n", str2, teamjersey,jerseyquality, jerseydesigntype, jerseysize, qtynum, tprice, local_time->tm_mon + 1, local_time->tm_mday, local_time->tm_year + 1900, local_time->tm_hour, local_time->tm_min);
                        sprintf(str3,"=======Details of  The Order=======\nUnique Code: %s \nTeam Name: %s \nJersey Quality: %s \nJersey Design: %s \nJersey Size: %s \nQuantity: %d \nTotal Price: %d TAKA \nOrder Time: %d/%d/%d %02d:%02d\n\n", str2, teamjersey,jerseyquality, jerseydesigntype, jerseysize, qtynum, tprice, local_time->tm_mon + 1, local_time->tm_mday, local_time->tm_year + 1900, local_time->tm_hour, local_time->tm_min);


                    }
                }
                else if(sizenum==5){

                    printf("Size: Extra Large");
                    strcpy(jerseysize, "Extra Large.");
                    printf("\n\nEnter The Quantity: ");
                    scanf("%d", &qtynum);
                    if(qtynum>0){

                        time_t current_time = time(NULL);
                        struct tm *local_time = localtime(&current_time);
                        tprice= qtynum*price;
                        printf("=======Details of  The Order=======\nUnique Code: %s \nTeam Name: %s \nJersey Quality: %s \nJersey Design: %s \nJersey Size: %s \nQuantity: %d \nTotal Price: %d TAKA \nOrder Time: %d/%d/%d %02d:%02d\n\n", str2, teamjersey,jerseyquality, jerseydesigntype, jerseysize, qtynum, tprice, local_time->tm_mon + 1, local_time->tm_mday, local_time->tm_year + 1900, local_time->tm_hour, local_time->tm_min);
                        sprintf(str3,"=======Details of  The Order=======\nUnique Code: %s \nTeam Name: %s \nJersey Quality: %s \nJersey Design: %s \nJersey Size: %s \nQuantity: %d \nTotal Price: %d TAKA \nOrder Time: %d/%d/%d %02d:%02d\n\n", str2, teamjersey,jerseyquality, jerseydesigntype, jerseysize, qtynum, tprice, local_time->tm_mon + 1, local_time->tm_mday, local_time->tm_year + 1900, local_time->tm_hour, local_time->tm_min);



                    }
                }
                else if(sizenum==6){

                    printf("Size: Extra Extra Large");
                    strcpy(jerseysize, "Extra Extra Large.");
                    printf("\n\nEnter The Quantity: ");
                    scanf("%d", &qtynum);
                    if(qtynum>0){

                        time_t current_time = time(NULL);
                        struct tm *local_time = localtime(&current_time);
                        tprice= qtynum*price;
                        printf("=======Details of  The Order=======\nUnique Code: %s \nTeam Name: %s \nJersey Quality: %s \nJersey Design: %s \nJersey Size: %s \nQuantity: %d \nTotal Price: %d TAKA \nOrder Time: %d/%d/%d %02d:%02d\n\n", str2, teamjersey,jerseyquality, jerseydesigntype, jerseysize, qtynum, tprice, local_time->tm_mon + 1, local_time->tm_mday, local_time->tm_year + 1900, local_time->tm_hour, local_time->tm_min);
                        sprintf(str3,"=======Details of  The Order=======\nUnique Code: %s \nTeam Name: %s \nJersey Quality: %s \nJersey Design: %s \nJersey Size: %s \nQuantity: %d \nTotal Price: %d TAKA \nOrder Time: %d/%d/%d %02d:%02d\n\n", str2, teamjersey,jerseyquality, jerseydesigntype, jerseysize, qtynum, tprice, local_time->tm_mon + 1, local_time->tm_mday, local_time->tm_year + 1900, local_time->tm_hour, local_time->tm_min);



                    }
                }
                else
                    printf("Invalid!");
        }
        else
            printf("Invalid!");

    }
    else
        printf("Invalid!");

FILE *data_geeks;
data_geeks = fopen("jersey_geeks.txt", "a");
if(data_geeks== NULL)
    printf("\nNo Files");
else
    printf("\nFile created");
    fputs(str3, data_geeks);
    fclose(data_geeks);

