void main()

{
    int n;
    printf("Enter the limit: ");
    scanf("%d", &n);
    int i,x, flag;

    for(x=1; i<=n;x++)
    {
        flag=1;
        for(i=2; i<=x/2; i++){

            if (x%i==0){
                flag=0;
                break;
            }
        }
        if(flag==1){

            printf("%d is prime", x);
        }
